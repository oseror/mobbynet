<section id="serchber">
  <section class="serber-wapper">
    <section class="regester-img" style="float:left; margin-left: 400px;"><img src="/assets/Login.png"  alt="" /></section>

  </section>
  <!-- serchber-->    </section>


<section id="regesterbody">
  <section id="body-wapper">

    <h3 class="singup-text">Sign In Now</h3>

    <%= form_for(resource, :as => resource_name, :url => session_path(resource_name),:class=>"regester-form") do |f| %>
      <div>
        <%= f.label :email,:style=>"padding-right:28px;" %><%= f.email_field :email, :autofocus => true,:Placeholder=>"Your Email",:class=>"text-are-for-re" %></div>

      <p><%= f.label :password %>
        <%= f.password_field :password, :autofocus => true,:Placeholder=>"Your password",:class=>"text-are-for-re" %></p>
      <br/>
      <% if devise_mapping.rememberable? -%>
        <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
      <% end -%><br/>

      <div> <%= f.submit "Sign in",:class=>"signup-btn",:style=>"margin-left:5px" %> <input name="" type="button" value="Login With Facebook"  class="signup-btn" style="margin-left:8px"/>
</div>
    <% end %>

      <div style="margin-left:215px"><%= render "devise/shared/links" %></div>







  </section>

